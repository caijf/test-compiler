{
  "name": "test-compiler",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:es-babel && npm run build:es-tsc && npm run build:cjs-babel && npm run build:cjs-tsc",
    "build:es-babel": "rm -rf es-babel && cross-env MODULE_TYPE=es babel src -d es-babel --extensions \".ts\"",
    "build:es-tsc": "rm -rf es-tsc && tsc -t ES5 -m ESNext --outDir ./es-tsc",
    "build:cjs-babel": "rm -rf cjs-babel && cross-env MODULE_TYPE=cjs babel src -d cjs-babel --extensions \".ts\"",
    "build:cjs-tsc": "rm -rf cjs-tsc && tsc -t ES5 -m commonjs --outDir ./cjs-tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "cz"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "4"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "tslib": "^2.2.0"
  }
}